<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                               
    <TableSelectDialog                                                                                                                                                                                                                                         
        noDataText="No Transports Found"                                                                                                                                                                                                                       
        title="Select Transport Request"                                                                                                                                                                                                                       
        confirm=".handleTRValueHelpClose"                                                                                                                                                                                                                      
        cancel=".handleTRValueHelpClose"                                                                                                                                                                                                                       
        search=".handleSearchTR"                                                                                                                                                                                                                               
        searchPlaceholder="Search Transport Requests"                                                                                                                                                                                                          
        showClearButton="false"                                                                                                                                                                                                                                
        resizable="true"                                                                                                                                                                                                                                       
        draggable="true"                                                                                                                                                                                                                                       
        contentWidth="800px"                                                                                                                                                                                                                                   
        contentHeight="400px"                                                                                                                                                                                                                                  
        class="sapUiResponsivePadding--header sapUiResponsivePadding--subHeader sapUiResponsivePadding--content sapUiResponsivePadding--footer"                                                                                                                
        items="{                                                                                                                                                                                                                                               
			path : 'translation>/TransportRequests',                                                                                                                                                                                                                    
			sorter : {                                                                                                                                                                                                                                                  
				path : 'Trkorr',                                                                                                                                                                                                                                           
				descending : false                                                                                                                                                                                                                                         
			}                                                                                                                                                                                                                                                           
		}"                                                                                                                                                                                                                                                           
    >                                                                                                                                                                                                                                                          
        <ColumnListItem selected="{selected}">                                                                                                                                                                                                                 
            <cells>                                                                                                                                                                                                                                            
                <ObjectIdentifier title="{translation>Trkorr}" />                                                                                                                                                                                              
                <Text text="{translation>Tarsystem}" />                                                                                                                                                                                                        
                <Text text="{translation>As4text}" />                                                                                                                                                                                                          
                <Text text="{path:'translation>As4date', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy' , UTC: true }}" />                                                                                                             
                <Text text="{path:'translation>As4time', type: 'sap.ui.model.odata.type.Time', formatOptions: { pattern: 'ss:mm:HH' , UTC: true }}" />                                                                                                         
            </cells>                                                                                                                                                                                                                                           
        </ColumnListItem>                                                                                                                                                                                                                                      
        <columns>                                                                                                                                                                                                                                              
            <Column width="15%">                                                                                                                                                                                                                               
                <header>                                                                                                                                                                                                                                       
                    <Text text="Transport" />                                                                                                                                                                                                                  
                </header>                                                                                                                                                                                                                                      
            </Column>                                                                                                                                                                                                                                          
            <Column width="10%">&nbsp;                                                                                                                                                                                                                         
                <header>                                                                                                                                                                                                                                       
                    <Text text="Target" />                                                                                                                                                                                                                     
                </header>                                                                                                                                                                                                                                      
            </Column>                                                                                                                                                                                                                                          
            <Column width="45%">                                                                                                                                                                                                                               
                <header>                                                                                                                                                                                                                                       
                    <Text text="Description" />                                                                                                                                                                                                                
                </header>                                                                                                                                                                                                                                      
            </Column>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
            <Column>                                                                                                                                                                                                                                           
                <header>                                                                                                                                                                                                                                       
                    <Text text="Date" />                                                                                                                                                                                                                       
                </header>                                                                                                                                                                                                                                      
            </Column>                                                                                                                                                                                                                                          
            <Column>                                                                                                                                                                                                                                           
                <header>                                                                                                                                                                                                                                       
                    <Text text="Time" />                                                                                                                                                                                                                       
                </header>                                                                                                                                                                                                                                      
            </Column>                                                                                                                                                                                                                                          
        </columns>                                                                                                                                                                                                                                             
    </TableSelectDialog>                                                                                                                                                                                                                                       
</core:FragmentDefinition>                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               